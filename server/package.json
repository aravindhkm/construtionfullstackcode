{
	"name": "construction",
	"version": "0.0.1",
	"description": "Construction Admin Service",
	"author": "Siva",
	"main": "index.js",
	"private": false,
	"engines": {
		"node": ">=10.15.0",
		"npm": ">=6.4.1",
		"yarn": ">=1.17.3"
	},
	"scripts": {
		"start": "gulp serve",
		"start:debug": "cross-env DEBUG=cyou-rest-api:* yarn start",
		"build": "gulp",
		"copy": "gulp copy",
		"babel": "gulp babel",
		"lint": "esw *.js src config --color",
		"lint-fix": "esw *.js src config --color --fix",
		"lint:watch": "yarn lint -- --watch",
		"fix-js": "eslint-auto-fix \"src/**/.js\" \"config/**/.js\" \"config/*.js\"",
		"beautify": "es-beautifier -u -c .eslintrc.js *.js src",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _jest -- --ui bdd --reporter spec --colors --compilers js:babel-core/register server/tests --recursive",
		"test:check-coverage": "yarn test:coverage && istanbul check-coverage",
		"report-coverage": "coveralls < ./coverage/lcov.info"
	},
	"keywords": [
		"express",
		"node",
		"node.js",
		"mongodb",
		"mongoose",
		"es6",
		"jest",
		"istanbul",
		"REST",
		"API",
		"boilerplate"
	],
	"jest": {
		"automock": false,
		"bail": false,
		"verbose": true,
		"setupFiles": [
			"./jest-setup.js"
		],
		"testMatch": [
			"**/Tests/**/*.js",
			"**/?(*.)(spec|test|integration).js?(x)"
		],
		"testPathIgnorePatterns": [
			"/node_modules/",
			"/coverage/",
			"/dist/"
		],
		"testEnvironment": "node"
	},
	"dependencies": {
		"@babel/runtime": "^7.7.6",
		"@hapi/joi": "^17.1.1",
		"archiver": "^5.0.0",
		"async": "^3.2.0",
		"bcrypt": "^3.0.8",
		"bluebird": "3.5.5",
		"body-parser": "1.19.0",
		"compression": "1.7.4",
		"cookie-parser": "1.4.4",
		"cors": "2.8.5",
		"crypto": "^1.0.1",
		"debug": "^4.1.1",
		"del": "^5.0.0",
		"dotenv": "^8.2.0",
		"download-file": "^0.1.5",
		"express": "4.17.1",
		"express-jwt": "5.3.1",
		"express-validation": "^3.0.2",
		"express-validator": "^6.3.0",
		"express-winston": "3.3.0",
		"fcm-node": "^1.5.2",
		"gulp": "4.0.2",
		"gulp-load-plugins": "^2.0.1",
		"handlebars": "^4.7.3",
		"helmet": "^3.21.2",
		"html-pdf": "^2.2.0",
		"http": "0.0.0",
		"http-status": "^1.3.2",
		"jsonwebtoken": "8.5.1",
		"lodash": "^4.17.15",
		"method-override": "^3.0.0",
		"moment": "^2.24.0",
		"multer": "^1.4.2",
		"nodemailer": "^6.4.2",
		"path": "^0.12.7",
		"pdf-creator-node": "^1.4.1",
		"pg": "^8.2.1",
		"pg-hstore": "^2.3.3",
		"request": "^2.88.0",
		"sequelize": "^5.21.10",
		"supertest": "4.0.2",
		"textlocal": "^0.1.1",
		"winston": "3.2.1"
	},
	"devDependencies": {
		"@babel/core": "^7.2.2",
		"@babel/plugin-proposal-export-default-from": "^7.2.0",
		"@babel/plugin-proposal-export-namespace-from": "^7.2.0",
		"@babel/plugin-proposal-function-bind": "^7.2.0",
		"@babel/plugin-proposal-object-rest-spread": "^7.5.5",
		"@babel/plugin-syntax-dynamic-import": "^7.2.0",
		"@babel/plugin-syntax-export-namespace-from": "^7.2.0",
		"@babel/plugin-transform-modules-commonjs": "^7.5.0",
		"@babel/plugin-transform-runtime": "^7.7.6",
		"@babel/preset-env": "^7.2.0",
		"@babel/preset-react": "^7.0.0",
		"@babel/register": "^7.0.0",
		"babel-eslint": "^10.0.2",
		"babel-loader": "^8.0.4",
		"commitizen": "^4.0.3",
		"coveralls": "^3.0.6",
		"cross-env": "5.2.0",
		"es-beautifier": "^1.2.0",
		"eslint": "^6.8.0",
		"eslint-config-airbnb-base": "14.0.0",
		"eslint-plugin-eslint-plugin": "^2.2.1",
		"eslint-plugin-import": "2.18.2",
		"eslint-plugin-jsdoc": "^22.0.1",
		"eslint-watch": "^6.0.1",
		"gulp-babel": "8.0.0",
		"gulp-newer": "^1.1.0",
		"gulp-nodemon": "^2.0.6",
		"gulp-sourcemaps": "^2.6.5",
		"husky": "^3.0.3",
		"istanbul": "1.1.0-alpha.1",
		"jest": "^25.1.0",
		"jest-fetch-mock": "2.1.2",
		"jsdom": "15.1.1"
	}
}
